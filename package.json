{
  "name": "@example/blog",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prepare": "husky install",
    "eslint": "npx eslint --fix .",
    "prettier": "npm prettier --write",
    "execute-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "lint": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint-fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "@astrojs/mdx": "^0.12.2",
    "@astrojs/prism": "^1.0.2",
    "@astrojs/react": "^1.2.2",
    "@astrojs/rss": "^1.2.1",
    "@astrojs/sitemap": "^1.0.0",
    "@astrojs/tailwind": "^2.1.3",
    "@emailjs/browser": "^3.10.0",
    "@heroicons/react": "^2.0.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "astro": "^1.6.15",
    "astro-icon": "^0.8.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "^3.2.4",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/react-slick": "^0.23.10",
    "eslint": "^8.31.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.2"
  },
  "lint-staged": {
    "**/*.{js,jsx, ts, tsx}": [
      "eslint . --fix",
      "prettier --write ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
